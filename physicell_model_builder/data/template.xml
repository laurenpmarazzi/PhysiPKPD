<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>-500</x_min>
		<x_max>500</x_max>
		<y_min>-500</y_min>
		<y_max>500</y_max>
		<z_min>-10</z_min>
		<z_max>10</z_max>
		<dx>20</dx>
		<dy>20</dy>
		<dz>20</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">7200</max_time> <!-- 5 days * 24 h * 60 min -->
		<time_units>min</time_units>
		<space_units>micron</space_units>
	
		<dt_diffusion units="min">0.01</dt_diffusion>
		<dt_mechanics units="min">0.1</dt_mechanics>
		<dt_phenotype units="min">6</dt_phenotype>	
	</overall>
	
	<parallel>
		<omp_num_threads>6</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>output</folder> <!-- use . for root --> 

		<full_data>
			<interval units="min">360</interval>
			<enable>true</enable>
		</full_data>
		
		<SVG>
			<interval units="min">60</interval>
			<enable>true</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<options>
		<legacy_random_points_on_sphere_in_divide>false</legacy_random_points_on_sphere_in_divide>
        <virtual_wall_at_domain_edge>false</virtual_wall_at_domain_edge>
	</options>	

	<microenvironment_setup>
		<variable name="substrate" units="dimensionless" ID="0">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">100000.0</diffusion_coefficient>
				<decay_rate units="1/min">10</decay_rate>  
			</physical_parameter_set>
			<initial_condition units="mmHg">0</initial_condition>
			<Dirichlet_boundary_condition units="mmHg" enabled="true">0</Dirichlet_boundary_condition>
<!-- use this block to set Dirichlet boundary conditions on individual boundaries --> 
			<Dirichlet_options>
				<boundary_value ID="xmin" enabled="false">0</boundary_value>
				<boundary_value ID="xmax" enabled="false">1</boundary_value>
				<boundary_value ID="ymin" enabled="false">0</boundary_value>
				<boundary_value ID="ymax" enabled="false">2</boundary_value>
				<boundary_value ID="zmin" enabled="false">0</boundary_value>
				<boundary_value ID="zmax" enabled="false">3</boundary_value>
			</Dirichlet_options>
 		</variable>
		
		<options>
			<calculate_gradients>true</calculate_gradients>
			<track_internalized_substrates_in_each_agent>true</track_internalized_substrates_in_each_agent>
			<!-- not yet supported --> 
			<initial_condition type="matlab" enabled="false">
				<filename>./config/initial.mat</filename>
			</initial_condition>
			<!-- not yet supported --> 
			<dirichlet_nodes type="matlab" enabled="false">
				<filename>./config/dirichlet.mat</filename>
			</dirichlet_nodes>
		</options>
	</microenvironment_setup>	
	
	<cell_definitions>
		<cell_definition name="default" ID="0">
			<phenotype>
				<cycle code="6" name="Flow cytometry model (separated)">  
					<!-- phases are: G0/G1 , S, G2, M --> 

					<!-- using higher than normal significant digits to match divisions in default code -->
					<!-- use phase_transition_rates OR phase_durations -->
					<!--
					<phase_transition_rates units="1/min"> 
						<rate start_index="0" end_index="1" fixed_duration="false">0.00334672</rate>
						<rate start_index="1" end_index="2" fixed_duration="true">0.00208333</rate>
						<rate start_index="2" end_index="3" fixed_duration="true">0.00416667</rate>
						<rate start_index="3" end_index="0" fixed_duration="true">0.0166667</rate>
					</phase_transition_rates>
					-->
					
					<!-- use phase_transition_rates OR phase_durations -->
					<phase_durations units="min"> 
						<duration index="0" fixed_duration="false">300.0</duration>
						<duration index="1" fixed_duration="true">480</duration>
						<duration index="2" fixed_duration="true">240</duration>
						<duration index="3" fixed_duration="true">60</duration>
					</phase_durations>
				</cycle>
				
				<death>  
					<model code="100" name="apoptosis"> 
						<death_rate units="1/min">5.31667e-05</death_rate>
						<!-- use phase_transition_rates OR phase_durations -->
						<!--
						<phase_transition_rates units="1/min">
							<rate start_index="0" end_index="1" fixed_duration="true">0.00193798</rate>
						</phase_transition_rates>
						-->
						<phase_durations units="min">
							<duration index="0" fixed_duration="true">516</duration>
						</phase_durations>
						<parameters>
							<unlysed_fluid_change_rate units="1/min">0.05</unlysed_fluid_change_rate>
							<lysed_fluid_change_rate units="1/min">0</lysed_fluid_change_rate>
							<cytoplasmic_biomass_change_rate units="1/min">1.66667e-02</cytoplasmic_biomass_change_rate>
							<nuclear_biomass_change_rate units="1/min">5.83333e-03</nuclear_biomass_change_rate>
							<calcification_rate units="1/min">0</calcification_rate>
							<relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
						</parameters>
					</model> 

					<model code="101" name="necrosis">
						<death_rate units="1/min">0.0</death_rate>
						<!-- use phase_transition_rates OR phase_durations -->
						<!-- necrosis uses phase_duration[0] = 0 so that it always immediately
							 tries to transition and instead checks volume against the rupture
							 volume --> 
<!--						
						<phase_transition_rates units="1/min">
							<rate start_index="0" end_index="1" fixed_duration="false">9e9</rate>
							<rate start_index="1" end_index="2" fixed_duration="true">1.15741e-5</rate>
						</phase_transition_rates>
-->						
						<phase_durations units="min">
							<duration index="0" fixed_duration="true">0</duration>
							<duration index="1" fixed_duration="true">86400</duration>
						</phase_durations>
						
						<parameters>
							<unlysed_fluid_change_rate units="1/min">0.05</unlysed_fluid_change_rate>
							<lysed_fluid_change_rate units="1/min">0</lysed_fluid_change_rate>
							<cytoplasmic_biomass_change_rate units="1/min">1.66667e-02</cytoplasmic_biomass_change_rate>
							<nuclear_biomass_change_rate units="1/min">5.83333e-03</nuclear_biomass_change_rate>
							<calcification_rate units="1/min">0</calcification_rate>
							<relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
						</parameters>
					</model> 
				</death>					

				<volume>  
					<total units="micron^3">2494</total>
					<fluid_fraction units="dimensionless">0.75</fluid_fraction>
					<nuclear units="micron^3">540</nuclear>
					
					<fluid_change_rate units="1/min">0.05</fluid_change_rate>
					<cytoplasmic_biomass_change_rate units="1/min">0.0045</cytoplasmic_biomass_change_rate>
					<nuclear_biomass_change_rate units="1/min">0.0055</nuclear_biomass_change_rate>
					
					<calcified_fraction units="dimensionless">0</calcified_fraction>
					<calcification_rate units="1/min">0</calcification_rate>
					
					<relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
				</volume> 				
				
				<mechanics> 
					<cell_cell_adhesion_strength units="micron/min">0.4</cell_cell_adhesion_strength>
					<cell_cell_repulsion_strength units="micron/min">10.0</cell_cell_repulsion_strength>
					<relative_maximum_adhesion_distance units="dimensionless">1.25</relative_maximum_adhesion_distance>
					
					<options>
						<set_relative_equilibrium_distance enabled="false" units="dimensionless">1.8</set_relative_equilibrium_distance>
						<set_absolute_equilibrium_distance enabled="false" units="micron">15.12</set_absolute_equilibrium_distance>
					</options>
				</mechanics>
				
				<motility>  
					<speed units="micron/min">1</speed>
					<persistence_time units="min">1</persistence_time>
					<migration_bias units="dimensionless">.5</migration_bias>
					
					<options>
						<enabled>false</enabled>
						<use_2D>true</use_2D>
						<chemotaxis>
							<enabled>false</enabled>
							<substrate>substrate</substrate>
							<direction>1</direction>
						</chemotaxis>
					</options>
				</motility>
				
				<secretion>
					<substrate name="substrate">
						<secretion_rate units="1/min">0</secretion_rate>
						<secretion_target units="substrate density">1</secretion_target>
						<uptake_rate units="1/min">0</uptake_rate>
						<net_export_rate units="total substrate/min">0</net_export_rate> 
					</substrate> 
				</secretion>

				<molecular />
			</phenotype>
			<pkpd_data>  
				<PKPD_D1 name="PKPD_D1">
					<PKPD_D1_moa_is_prolif>0.0</PKPD_D1_moa_is_prolif>
					<PKPD_D1_prolif_saturation_rate>1e-6</PKPD_D1_prolif_saturation_rate>
					<PKPD_D1_prolif_EC50>1e8</PKPD_D1_prolif_EC50>
					<PKPD_D1_prolif_hill_power>2</PKPD_D1_prolif_hill_power>

					<PKPD_D1_moa_is_apop>0.0</PKPD_D1_moa_is_apop>
					<PKPD_D1_apop_saturation_rate>1E-4</PKPD_D1_apop_saturation_rate>
					<PKPD_D1_apop_EC50>1e8</PKPD_D1_apop_EC50>
					<PKPD_D1_apop_hill_power>2</PKPD_D1_apop_hill_power>

					<PKPD_D1_moa_is_necrosis>0.0</PKPD_D1_moa_is_necrosis>
					<PKPD_D1_necrosis_saturation_rate>1E-4</PKPD_D1_necrosis_saturation_rate>
					<PKPD_D1_necrosis_EC50>1e8</PKPD_D1_necrosis_EC50>
					<PKPD_D1_necrosis_hill_power>2</PKPD_D1_necrosis_hill_power>

					<PKPD_D1_moa_is_motility>0.0</PKPD_D1_moa_is_motility>
					<PKPD_D1_motility_saturation_rate>1</PKPD_D1_motility_saturation_rate>
					<PKPD_D1_motility_EC50>1e8</PKPD_D1_motility_EC50>
					<PKPD_D1_motility_hill_power>2</PKPD_D1_motility_hill_power>

					<PKPD_D1_damage>0</PKPD_D1_damage>
					<PKPD_D1_repair_rate>1e8</PKPD_D1_repair_rate>
					<PKPD_D1_metabolism_rate>0.05</PKPD_D1_metabolism_rate>
				</PKPD_D1>

<!-- <div_drugs type="divider" description="-Starting Drug 2 PD parameters-"/> -->

				<PKPD_D2 name="PKPD_D2">
                <PKPD_D2_moa_is_prolif>0.0</PKPD_D2_moa_is_prolif>
                <PKPD_D2_prolif_saturation_rate>1E-6</PKPD_D2_prolif_saturation_rate>
                <PKPD_D2_prolif_EC50>1e10</PKPD_D2_prolif_EC50>
                <PKPD_D2_prolif_hill_power>2</PKPD_D2_prolif_hill_power>

                <PKPD_D2_moa_is_apop>0.0</PKPD_D2_moa_is_apop>
                <PKPD_D2_apop_saturation_rate>1e-2</PKPD_D2_apop_saturation_rate>
                <PKPD_D2_apop_EC50>1e8</PKPD_D2_apop_EC50>
                <PKPD_D2_apop_hill_power>2</PKPD_D2_apop_hill_power>

                <PKPD_D2_moa_is_necrosis>0.0</PKPD_D2_moa_is_necrosis>
                <PKPD_D2_necrosis_saturation_rate>1E-4</PKPD_D2_necrosis_saturation_rate>
                <PKPD_D2_necrosis_EC50>1e8</PKPD_D2_necrosis_EC50>
                <PKPD_D2_necrosis_hill_power>2</PKPD_D2_necrosis_hill_power>

                <PKPD_D2_moa_is_motility>0.0</PKPD_D2_moa_is_motility>
                <PKPD_D2_motility_saturation_rate>1</PKPD_D2_motility_saturation_rate>
                <PKPD_D2_motility_EC50>1e8</PKPD_D2_motility_EC50>
                <PKPD_D2_motility_hill_power>2</PKPD_D2_motility_hill_power>

                <PKPD_D2_damage>0</PKPD_D2_damage>
                <PKPD_D2_repair_rate>5e6</PKPD_D2_repair_rate>
                <PKPD_D2_metabolism_rate>0.05</PKPD_D2_metabolism_rate>
				</PKPD_D2>

			</pkpd_data>
			<custom_data>  
				<sample units="dimensionless">1.0</sample>
			</custom_data>
		</cell_definition>
	</cell_definitions>
	
	<initial_conditions>
		<cell_positions type="csv" enabled="false">
			<folder>./config</folder>
			<filename>cells.csv</filename>
		</cell_positions>
	</initial_conditions>	
	
	<user_parameters>
<div_initialization type="divider" description="---Initialization settings---"/>

        <random_seed type="int" units="dimensionless" description="">0</random_seed>
        <number_of_tumor_1_cells type="int" units="none" description="initial number of tumor_1 cells">500</number_of_tumor_1_cells>
        <number_of_tumor_2_cells type="int" units="none" description="initial number of tumor_2 cells">0</number_of_tumor_2_cells>
        <csv_data_interval type="double" units="min" description="time between writing data to CSV">100</csv_data_interval>
        <!-- <max_initial_distance type="double" units="micron" description="max initial cell distance from origin">250.0</max_initial_distance> -->




<div_drug1 type="divider" description="---Drug 1 Settings---"/>

        <PKPD_D1_number_loading_doses type="int" units="none" description="number of loading doses of drug 1">0</PKPD_D1_number_loading_doses>
        <PKPD_D1_max_number_doses type="int" units="none" description="max doses of drug 1, including loading doses">1</PKPD_D1_max_number_doses>
        <PKPD_D1_dose_interval type="double" units="min" description="time between dosings of drug 1">360.0</PKPD_D1_dose_interval>
        <PKPD_D1_set_first_dose_time type="bool" units="none" description="user sets first dose time to start therapy for drug 1; false=user gives confluence condition">True</PKPD_D1_set_first_dose_time>
        <PKPD_D1_first_dose_time type="double" units="min" description="time of first dose for drug 1">0</PKPD_D1_first_dose_time>
        <PKPD_D1_confluence_condition type="double" units="none" description="confluence as proportion at which to start therapy for drug 1">0.6</PKPD_D1_confluence_condition>
        <d1_color_ec50 type="double" units="none" description="ec50 used for coloring">1e8</d1_color_ec50>
        <d1_color_hp type="double" units="none" description="hill power used for coloring">2.0</d1_color_hp>



<div_drug1_PK type="divider" description="---Drug 1 PK---"/>

        <PKPD_D1_central_increase_on_loading_dose type="double" units="mmHg" description="concentration added to systemic circulation on loading dose of drug 1">400.0</PKPD_D1_central_increase_on_loading_dose>
        <PKPD_D1_central_increase_on_dose type="double" units="mmHg" description="concentration added to systemic circulation on dose of drug 1">2000.0</PKPD_D1_central_increase_on_dose>
        <PKPD_D1_central_elimination_rate type="double" units="1/min" description="decay rate of drug 1 in systemic circulation">0.0027</PKPD_D1_central_elimination_rate>
        <PKPD_D1_flux_across_capillaries type="double" units="1/min" description="rate of central concentration change of drug 1 due to distribution and redistribution">0.0048</PKPD_D1_flux_across_capillaries>
        <PKPD_D1_biot_number type="double" units="dimensionless" description="ratio of drug concentration outside capillary to inside capillary (or just all systemic circulation">1.0</PKPD_D1_biot_number>



<div_drug2 type="divider" description="---Drug 2 Settings---"/>

        <PKPD_D2_number_loading_doses type="int" units="none" description="number of loading doses of drug 2">0</PKPD_D2_number_loading_doses>
        <PKPD_D2_max_number_doses type="int" units="none" description="max doses of drug 2, including loading doses">1</PKPD_D2_max_number_doses>
        <PKPD_D2_dose_interval type="double" units="min" description="time between dosings of drug 2">360.0</PKPD_D2_dose_interval>
        <PKPD_D2_set_first_dose_time type="bool" units="none" description="user sets first dose time to start therapy for drug 2; false=user gives confluence condition">True</PKPD_D2_set_first_dose_time>
        <PKPD_D2_first_dose_time type="double" units="min" description="time of first dose for drug 2">120</PKPD_D2_first_dose_time>
        <PKPD_D2_confluence_condition type="double" units="none" description="confluence as proportion at which to start therapy for drug 2">0.6</PKPD_D2_confluence_condition>
        <d2_color_ec50 type="double" units="none" description="ec50 used for coloring">1e8</d2_color_ec50>
        <d2_color_hp type="double" units="none" description="hill power used for coloring">2.0</d2_color_hp>



<div_drug2_PK type="divider" description="---Drug 2 PK---"/>

        <PKPD_D2_central_increase_on_loading_dose type="double" units="mmHg" description="concentration added to systemic circulation on loading dose of drug 2">400.0</PKPD_D2_central_increase_on_loading_dose>
        <PKPD_D2_central_increase_on_dose type="double" units="mmHg" description="concentration added to systemic circulation on dose of drug 2">50000.0</PKPD_D2_central_increase_on_dose>
        <PKPD_D2_central_elimination_rate type="double" units="1/min" description="decay rate of drug 2 in systemic circulation">0.0027</PKPD_D2_central_elimination_rate>
        <PKPD_D2_flux_across_capillaries type="double" units="1/min" description="rate of central concentration change of drug 2 due to distribution and redistribution">0.0048</PKPD_D2_flux_across_capillaries>
        <PKPD_D2_biot_number type="double" units="dimensionless" description="ratio of drug concentration outside capillary to inside capillary (or just all systemic circulation">1.0</PKPD_D2_biot_number>



<div_generalPK type="divider" description="---PK---"/>

        <central_to_periphery_volume_ratio type="double" units="dimensionless" description="ratio of volume in systemic circulation to the volume of the periphery">1.0</central_to_periphery_volume_ratio>

	</user_parameters>
	
</PhysiCell_settings>
